<?xml version="1.0"?>
<extension key="de.systopia.drupalalias" type="module">
  <file>drupalalias</file>
  <name>Drupal Aliases</name>
  <description>This extension replaces CiviCRM urls with their Drupal aliases.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <releaseDate>2016-09-13</releaseDate>
  <version>0.4</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.6</ver>
  </compatibility>
  <comments>Currently only active on: CRM/Campaign/Page/Petition/ThankYou.tpl</comments>
  <civix>
    <namespace>CRM/Drupalalias</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
